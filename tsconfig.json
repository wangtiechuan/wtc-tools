{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@wtc/wtc-common-tools": ["./packages/wtc-common-tools/src/index.ts"],
      "@wtc/wtc-node-tools": ["./packages/wtc-node-tools/src/index.ts"],
      "@wtc/wtc-web-tools": ["./packages/wtc-web-tools/src/index.ts"],
      "@wtc/wtc-react-comp": ["./packages/wtc-react-comp/src/index.ts"]
    }
  },
  "include": [
    ".dumi/**/*",
    ".dumirc.ts",
    "packages/*/src/**/*",
    "packages/*/docs/**/*"
  ]
}
