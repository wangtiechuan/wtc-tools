{
  "compilerOptions": {
    "strict": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "allowJs": true,
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@victor/victor-common-tools": [
        "./packages/wtc-common-tools/src/index.ts"
      ],
      "@victor/victor-node-tools": ["./packages/wtc-node-tools/src/index.ts"],
      "@victor/victor-web-tools": ["./packages/wtc-web-tools/src/index.ts"],
      "@victor/victor-react-comp": ["./packages/wtc-react-comp/src/index.ts"],
      "@victor/victor-go-database": ["./packages/wtc-go-database/src/index.ts"],
      "@victor/victor-exchange-api": [
        "./packages/wtc-exchange-api/src/index.ts"
      ],
      "@victor/victor-save-kline": ["./packages/wtc-save-kline/src/index.ts"],
      "@victor/victor-kline-strategy": [
        "./packages/wtc-kline-strategy/src/index.ts"
      ],

      "@victor/victor-exchange-go": ["./packages/wtc-exchange-go/src/index.ts"],
      "@victor/victor-message-push": [
        "./packages/wtc-message-push/src/index.ts"
      ]
    }
  },
  "include": [
    ".dumi/**/*",
    ".dumirc.ts",
    "packages/*/src/**/*",
    "packages/*/docs/**/*",
    "node_modules/ccxt/js/*"
  ]
}
